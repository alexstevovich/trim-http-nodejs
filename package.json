{
    "name": "trim-http",
    "version": "1.0.1",
    "homepage": "https://alexstevovich.com/a/trim-http-nodejs",
    "author": {
        "name": "Alex Stevovich",
        "email": "dev@alexstevovich.com",
        "url": "https://alexstevovich.com"
    },
    "description": "Removes the http:// or https:// prefix from a URL string",
    "type": "module",
    "main": "./src/index.js",
    "files": [
        "./src"
    ],
    "license": "MIT",
    "scripts": {
        "poshify": "npx poshify",
        "pretty": "npx prettier --write .",
        "lint": "eslint ./src/",
        "lint:fix": "eslint --fix ./src/",
        "test": "npx vitest --run",
        "build": "npm run poshify && npm run pretty && npm run lint:fix && npm run lint"
    },
    "keywords": [
        "url",
        "http",
        "https",
        "trim",
        "strip-protocol",
        "utility",
        "string",
        "normalize",
        "frontend",
        "web"
    ],
    "devDependencies": {
        "eslint": "^9.36.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "vitest": "^4.0.4"
    }
}
